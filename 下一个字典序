#include<bits/stdc++.h>
using namespace std;
int main() {
	int n; cin >> n;
	int* a = new int[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	int left = n - 1;
	while (a[left - 1] > a[left])
		left--;
	for (int i = n - 1; i >= left; i--)
		if (a[i] > a[left - 1]) {
			swap(a[i], a[left - 1]);
			break;
		}
	int L = n-left;
	for (int i = 1; i <= L/2; i++) 
		swap(a[n - i], a[n - L + i - 1]);
	for (int i = 0; i < n; i++)
		cout << a[i] << ' ';
	return 0;
}
