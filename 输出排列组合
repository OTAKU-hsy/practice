#include<bits/stdc++.h>
using namespace std;
bool v[100];
int a[100],n,r;

void pailie(int step) {
	if (step - 1 == r) {
		for (int i = 1;i <= r;i++)
			cout << a[i] << ' ';
		cout << endl;
		return;
	}
	for (int i = 1;i <= n;i++) {
		if (!v[i]) {
			a[step] = i;
			v[i] = 1;
			pailie(step + 1);
			v[i] = 0;
		}
	}
}

void zuhe(int f, int t) {
	a[t] = f;
	for (int i = 1; f + i - 1 <= n; i++) {
		if (t == r) {
			for (int j = 1; j <= r; j++)
				cout << setw(3) << a[j];
			cout << endl;
			return;
		}
		cs(f + i, t + 1);
	}
}

int main()
{
	cin >> n >> r;
	pailie(1);
	for (int i = 1; i + r - 1 <= n; i++)
		zuhe(i, 1);
	return 0;
}
