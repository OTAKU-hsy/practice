#include<bits/stdc++.h>
using namespace std;
void merge(int a[], int l, int r) {
	int m = (l + r) / 2;
	int n = r - l + 1;
	int* b = new int[n];
	int k = 0, i = l, j = m + 1;
	while (i <= m && j <= r) {
		if (a[i] >= a[j])
			b[k++] = a[j++];
		else
			b[k++] = a[i++];
	}
	if (i == m + 1)
		while (j <= r)
			b[k++] = a[j++];
	else
		while (i <= m)
			b[k++] = a[i++];
	k = 0;
	for (int i = l; i <= r; i++)
		a[i] = b[k++];
	delete[]b;
}
void merge_sort(int a[],int low,int high) {
	if (low < high) {
		int i = (low + high) / 2;
		merge_sort(a, low, i);
		merge_sort(a, i + 1, high);
		merge(a, low, high);
	}
}
int main() {
	int n; cin >> n;
	int* a = new int[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	merge_sort(a, 0, n-1);
	for (int i = 0; i < n; i++)
		cout << a[i];
	return 0;
}
//以上为归并排序
