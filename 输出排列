#include<bits/stdc++.h>
using namespace std;
bool v[100];
int a[100],n,r;

void cs(int step) {
	if (step - 1 == r) {
		for (int i = 1;i <= r;i++)
			cout << a[i] << ' ';
		cout << endl;
		return;
	}
	for (int i = 1;i <= n;i++) {
		if (!v[i]) {
			a[step] = i;
			v[i] = 1;
			cs(step + 1);
			v[i] = 0;
		}
	}
}

int main()
{
	cin >> n >> r;
	cs(1);
	return 0;
}
